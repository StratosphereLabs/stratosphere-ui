{
  "name": "stratosphere-ui",
  "version": "1.0.0",
  "description": "React component library for all Stratosphere Labs user interfaces",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/stratosphere-ui.js",
  "module": "./dist/stratosphere-ui.js",
  "exports": {
    ".": {
      "types": "./dist/stratosphere-ui.d.ts",
      "import": "./dist/stratosphere-ui.js",
      "require": "./dist/stratosphere-ui.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StratosphereLabs/stratosphere-ui.git"
  },
