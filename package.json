{
  "name": "stratosphere-ui",
  "version": "1.3.0",
  "description": "React component library for all Stratosphere Labs user interfaces",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/stratosphere-ui.js",
  "module": "./dist/stratosphere-ui.js",
  "exports": {
    ".": {
      "types": "./dist/stratosphere-ui.d.ts",
      "import": "./dist/stratosphere-ui.js",
      "require": "./dist/stratosphere-ui.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StratosphereLabs/stratosphere-ui.git"
  },
  "author": "Ethan Shields",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StratosphereLabs/stratosphere-ui/issues"
  },
  "homepage": "https://github.com/StratosphereLabs/stratosphere-ui#readme",
  "scripts": {
    "start": "npm run storybook",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "deploy-storybook": "cross-env NODE_OPTIONS=--max-old-space-size=6144 storybook-to-ghpages",
    "prepack": "npm run build",
    "format": "prettier --config ./.prettierrc -w src vite.config.ts",
    "test": "vitest run --coverage"
  },
  "devDependencies": {
    "@headlessui/react": "1.7.17",
